{
  "name": "nox.reference.getdata.node",
  "version": "1.0.0",
  "description": "Script to get holidays data for a selected year from 'date-holidays' library",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "holidays-2023": "ts-node Holidays/index.ts --year 2023 --file ../../data/input/source/Holidays/Nox.Reference.Holidays-2023.json",
    "holidays-2024": "ts-node Holidays/index.ts --year 2024 --file ../../data/input/source/Holidays/Nox.Reference.Holidays-2024.json",
    "holidays-2025": "ts-node Holidays/index.ts --year 2025 --file ../../data/input/source/Holidays/Nox.Reference.Holidays-2025.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.15.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "date-holidays": "^3.19.0",
    "luxon": "^3.3.0",
    "ts-node": "^10.9.1",
    "zlib": "^1.0.5"
  }
}
